@objects
	custom-element-standard			#custom-element-standard
	custom-element-standard-focus	#custom-element-standard-focus
	custom-element-main				#custom-element-main
	custom-element-small			#custom-element-small
	custom-element-main-small		#custom-element-main-small
	polymer-mixin-standard			#polymer-mixin-standard
	polymer-mixin-standard-focus	#polymer-mixin-standard-focus
	polymer-mixin-main				#polymer-mixin-main
	polymer-mixin-small				#polymer-mixin-small
	polymer-mixin-main-small		#polymer-mixin-main-small
	sass-mixin-standard				#sass-mixin-standard
	sass-mixin-standard-focus		#sass-mixin-standard-focus
	sass-mixin-main					#sass-mixin-main
	sass-mixin-small				#sass-mixin-small
	sass-mixin-main-small			#sass-mixin-main-small

@groups
	standard custom-element-standard, polymer-mixin-standard, sass-mixin-standard
	standard-focus custom-element-standard-focus, polymer-mixin-standard-focus, sass-mixin-standard-focus
	main custom-element-main, polymer-mixin-main, sass-mixin-main
	small custom-element-small, polymer-mixin-small, sass-mixin-small,
	main-small custom-element-main-small, polymer-mixin-main-small, sass-mixin-main-small

= Links =
	&standard:
		css color matches "#006fbf|rgba\\(0, 111, 191, 1\\)|rgb\\(0, 111, 191\\)"
		css font-size is "20px"
		css font-weight matches "normal|400"
		css text-decoration matches "none|none solid rgb\\(0, 111, 191\\)"
		css cursor is "pointer"
	&standard-focus:
		css color matches "#005694|rgba\\(0, 86, 148, 1\\)|rgb\\(0, 86, 148\\)"
		css font-size is "20px"
		css font-weight matches "normal|400"
		css text-decoration matches "underline|underline solid rgb\\(0, 86, 148\\)"
		css outline-width is "0px"
		css cursor is "pointer"
	&main:
		css color matches "#006fbf|rgba\\(0, 111, 191, 1\\)|rgb\\(0, 111, 191\\)"
		css font-size is "20px"
		css font-weight matches "bold|700"
		css text-decoration matches "none|none solid rgb\\(0, 111, 191\\)"
		css cursor is "pointer"
	&small:
		css color matches "#006fbf|rgba\\(0, 111, 191, 1\\)|rgb\\(0, 111, 191\\)"
		css font-size is "14px"
		css font-weight matches "normal|400"
		css letter-spacing matches "0\\.2[0-9]*px"
		css line-height matches "21px"
		css text-decoration matches "none|none solid rgb\\(0, 111, 191\\)"
		css cursor is "pointer"
	&main-small:
		css color matches "#006fbf|rgba\\(0, 111, 191, 1\\)|rgb\\(0, 111, 191\\)"
		css font-size is "14px"
		css font-weight matches "bold|700"
		css letter-spacing matches "0\\.2[0-9]*px"
		css line-height matches "21px"
		css text-decoration matches "none|none solid rgb\\(0, 111, 191\\)"
		css cursor is "pointer"
