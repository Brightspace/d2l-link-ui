@objects
	normal-*		.normal
	normal-focus-*	.d2l-link-focus
	main-*			.main
	small-*			.small
	main-small-*	.main-small

@groups
	normals normal-1, normal-2
	normal-focuses normal-focus-1, normal-focus-2
	mains main-1, main-2
	smalls small-1, small-2
	main-smalls main-small-1, main-small-2

= Links =
	&normals:
		css color matches "#006fbf|rgba\\(0, 111, 191, 1\\)|rgb\\(0, 111, 191\\)"
		css font-size is "20px"
		css font-weight matches "normal|400"
		css text-decoration matches "none|none solid rgb\\(0, 111, 191\\)"
		css cursor is "pointer"
	&normal-focuses:
		css color matches "#1c5295|rgba\\(28, 82, 149, 1\\)|rgb\\(28, 82, 149\\)"
		css font-size is "20px"
		css font-weight matches "normal|400"
		css text-decoration matches "underline|underline solid rgb\\(28, 82, 149\\)"
		css outline-width is "0px"
		css cursor is "pointer"
	&mains:
		css color matches "#006fbf|rgba\\(0, 111, 191, 1\\)|rgb\\(0, 111, 191\\)"
		css font-size is "20px"
		css font-weight matches "bold|700"
		css text-decoration matches "none|none solid rgb\\(0, 111, 191\\)"
		css cursor is "pointer"
	&smalls:
		css color matches "#006fbf|rgba\\(0, 111, 191, 1\\)|rgb\\(0, 111, 191\\)"
		css font-size is "14px"
		css font-weight matches "normal|400"
		css letter-spacing matches "0\\.2[0-9]*px"
		css line-height matches "21px"
		css text-decoration matches "none|none solid rgb\\(0, 111, 191\\)"
		css cursor is "pointer"
	&main-smalls:
		css color matches "#006fbf|rgba\\(0, 111, 191, 1\\)|rgb\\(0, 111, 191\\)"
		css font-size is "14px"
		css font-weight matches "bold|700"
		css letter-spacing matches "0\\.2[0-9]*px"
		css line-height matches "21px"
		css text-decoration matches "none|none solid rgb\\(0, 111, 191\\)"
		css cursor is "pointer"
